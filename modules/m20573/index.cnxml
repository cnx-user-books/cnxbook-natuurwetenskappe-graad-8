    <section id="id8841132">
      <title>Memorandum</title>
      <para id="id10306402">ELEKTRIESE TAAL</para>
      <para id="id7312413">Aktiwiteit</para>
      <para id="id3549085">Opdrag 1:</para>
      <para id="id18506775"> Daar is ‘n klomp woorde en terme wat leerders moet ken om verdere studies te kan doen</para>
      <table id="id3915788" summary="">
        <tgroup cols="3">
          <colspec colnum="1" colname="c1"/>
          <colspec colnum="2" colname="c2"/>
          <colspec colnum="3" colname="c3"/>
